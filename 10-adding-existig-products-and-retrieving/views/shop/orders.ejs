<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length<=0) {%>
                <h1>Nothing there!</h1>
            <% } else { %>
                <ul>
                    <% orders.forEach(order=>{ %>
                        <li>
                            <h1>#<%= orders.id %></h1>
                            <% order.products.forEach(product=>{ %>
                                <li>
                                    <h1>#<%= product.title%></h1>
                                    <h1>#<%= product.order_item.quantity %></h1>
                                </li>
                                <% }); %>  
                        </li>
                        <% }); %>  
                </ul>    
             <% } %> 
        </main>
        <%- include('../includes/end.ejs') %>